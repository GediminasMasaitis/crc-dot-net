<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard1.3;net35</TargetFrameworks>
    <PackageId>CrcDotNET</PackageId>
    <Authors>Gediminas Masaitis</Authors>
    <Owners>Gediminas Masaitis</Owners>
    <Company />
    <Product>Crc.NET</Product>
    <Title>Crc.NET</Title>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/GediminasMasaitis/crc-dot-net</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/GediminasMasaitis/crc-dot-net/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/GediminasMasaitis/crc-dot-net</RepositoryUrl>
    <Copyright>Copyright 2018</Copyright>
    <PackageTags>crc</PackageTags>
    <Description>.NET Standard library for calculating any CRC checksums</Description>
    <Summary>.NET Standard library for calculating any CRC checksums</Summary>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Version>1.0.3</Version>
    <AssemblyVersion>1.0.3.0</AssemblyVersion>
  </PropertyGroup>

</Project>
